
<div class="collapse navbar-collapse" id="bs-megadropdown-tabs">
    <ul class="nav navbar-nav nav_1">
        @foreach (var item in ViewBag.ProductGroup)
        {
        <li class="dropdown mega-dropdown">
            <a class="dropdown-toggle" data-toggle="dropdown">@item.ProductGroupName<span class="caret"></span></a>
            @{ Html.RenderAction("ChildrenMenu", "Home", new { parentId = item.ID});}
        </li>
        }
    </ul>
</div><!-- /.navbar-collapse -->
